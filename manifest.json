{
  "name": "Design Analyser!",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Extension to peek into current webpage.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Peek into Page design and visuals"
  },
  "permissions": [
  "activeTab",
  "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["popup.js", "contentscript.js"],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}


